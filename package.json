{
  "name": "node-graph",
  "author": "TiO2",
  "version": "0.0.0",
  "main": "index.js",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "packager": "electron-packager ./ testapp --win --out ./outputs --electron-version=28.2.4",
    "preview": "vite preview",
    "build": "vite build",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "electron:serve": "concurrently -k \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "vite build && electron-builder --win"
  },
  "build": {
    "appId": "com.example.yourapp",
    "productName": "Node Graph",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/**/*",
      "index.js",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rete": "^2.0.6",
    "rete-area-plugin": "^2.1.3",
    "rete-connection-plugin": "^2.0.5",
    "rete-react-plugin": "^2.0.7",
    "rete-render-utils": "^2.0.3",
    "rete-vue-plugin": "^2.1.2",
    "styled-components": "^6.1.19",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.2.0",
    "electron": "^37.2.6",
    "electron-packager": "^17.1.2",
    "npm-run-all2": "^8.0.4",
    "sass-embedded": "^1.90.0",
    "typescript": "~5.8.0",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vue-tsc": "^3.0.4",
    "wait-on": "^8.0.4"
  }
}
